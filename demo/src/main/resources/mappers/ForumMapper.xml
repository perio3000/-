<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.ForumMapper">

    <select id="getList" resultType="com.example.demo.vo.ArticleVO">
        <![CDATA[
            SELECT * FROM FORUM
        ]]>

    </select>

    <select id="read_content" resultType="com.example.demo.vo.ArticleVO">
        <![CDATA[
        SELECT * FROM FORUM WHERE  ID = #{id}
        ]]>
    </select>

    <select id="edit_content" resultType="com.example.demo.vo.ArticleVO">
        <![CDATA[
        SELECT * FROM FORUM WHERE  ID = #{id}
        ]]>
    </select>



</mapper>